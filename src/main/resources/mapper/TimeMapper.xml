<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springbootmybatis.dao.TimeMapper">

    <insert id="insertOne">
        insert into time (date, datetime) VALUES (#{date}, #{datetime})
    </insert>

    <insert id="insertOneOld">
        insert into time (date, datetime) VALUES (#{date, jdbcType=DATE}, #{datetime,  jdbcType=TIMESTAMP})
    </insert>
</mapper>